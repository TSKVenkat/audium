{
  "name": "audium-ai",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "node scripts/seed-db.js",
    "db:setup": "node scripts/database-setup.js",
    "db:indexes": "node -e \"require('./src/lib/mongodb-indexes').createOptimizedIndexes().then(() => process.exit(0))\"",
    "db:ttl": "node -e \"require('./src/lib/mongodb-ttl').initializeTTL().then(() => process.exit(0))\"",
    "db:cleanup": "node -e \"require('./src/lib/mongodb-ttl').TTLManager.performManualCleanup(false).then(r => { console.log(r); process.exit(0) })\"",
    "db:health": "node -e \"require('./src/lib/mongodb-connection-pool').mongoHealthCheck().then(r => { console.log(JSON.stringify(r, null, 2)); process.exit(0) })\"",
    "db:sharding": "node -e \"require('./src/lib/mongodb-sharding').initializeSharding().then(r => { console.log(r); process.exit(0) })\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.58.0",
    "@aws-sdk/client-polly": "^3.859.0",
    "@elevenlabs/elevenlabs-js": "^2.8.0",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@google-cloud/text-to-speech": "^6.2.0",
    "@google/generative-ai": "^0.21.0",
    "@stripe/stripe-js": "^2.4.0",
    "@tanstack/react-query": "^5.61.5",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0",
    "cohere-ai": "^7.18.0",
    "csv-parser": "^3.0.0",
    "ffmpeg-static": "^5.2.0",
    "formidable": "^3.5.2",
    "framer-motion": "^12.0.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.468.0",
    "mammoth": "^1.8.0",
    "mongodb": "^6.8.0",
    "next": "15.4.5",
    "openai": "^5.12.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.54.0",
    "react-hot-toast": "^2.4.1",
    "say": "^0.16.0",
    "sharp": "^0.34.3",
    "stripe": "^14.15.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "typescript": "^5"
  }
}
